{
  "name": "deadshot-viewer",
  "version": "1.0.0",
  "description": "Omniverse Client",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dist-win": "electron-packager . omniverse --platform=win32 --arch=x64 --out=../omniverse-dist --overwrite --no-asar",
    "dist-linux": "electron-packager . omniverse --platform=linux --arch=x64 --out=dist --overwrite --no-asar"
  },

  "devDependencies": {
    "electron": "^38.7.2",
    "@electron/packager": "^19.0.1"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0",
    "discord-rpc": "^4.0.1"
  }
}
